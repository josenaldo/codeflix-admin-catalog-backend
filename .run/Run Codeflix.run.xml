<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="Run Codeflix" type="SpringBootApplicationConfigurationType"
      factoryName="Spring Boot">
        <option name="ACTIVE_PROFILES" value="prod"/>
        <module name="admin-catalog-backend.infrastructure.main"/>
        <option name="SPRING_BOOT_MAIN_CLASS"
          value="br.com.josenaldo.codeflix.infrastructure.Application"/>
        <extension name="coverage">
            <pattern>
                <option name="PATTERN" value="br.com.josenaldo.codeflix.infrastructure.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>
